                <StackPanel Height="20" Margin ="0, 10, 0, 0" Orientation="Horizontal">
                    <Label x:Name="lbl_cmd_5" Height="20" Width="140" Content="Ребаф" HorizontalAlignment="Left" VerticalAlignment="Top" RenderTransformOrigin="0.368,-0.654" Padding="0" FontSize="14"/>
                    <TextBox x:Name="rebaf" Height="20" Width="70" Padding="0" Text="{Binding CheckBoxSettings.Rebaf}"></TextBox>
                </StackPanel>
                <StackPanel Height="20" Margin ="0, 10, 0, 0" Orientation="Horizontal">
                    <Label x:Name="lbl_cmd_6" Height="20" Width="140" Content="Следовать за целью" HorizontalAlignment="Left" VerticalAlignment="Top" RenderTransformOrigin="0.368,-0.654" Padding="0" FontSize="14"/>
                    <TextBox x:Name="to_him" Height="20" Width="70" Padding="0" Text="{Binding CheckBoxSettings.To_him}"></TextBox>
                </StackPanel>
                <StackPanel Height="20" Margin ="0, 10, 0, 0" Orientation="Horizontal">
                    <Label x:Name="lbl_cmd_7" Height="20" Width="140" Content="Остановиться" HorizontalAlignment="Left" VerticalAlignment="Top" RenderTransformOrigin="0.368,-0.654" Padding="0" FontSize="14"/>
                    <TextBox x:Name="stop" Height="20" Width="70" Padding="0" Text="{Binding CheckBoxSettings.Stop}"></TextBox>
                </StackPanel>
            </StackPanel>
        </GroupBox>
        <!-- Кнопка для обновления списка доступных персонажей -->
        <Button x:Name="button" Content="Обновить список окон" HorizontalAlignment="Left" Margin="396,338,0,0" VerticalAlignment="Top" Width="225" Height="25" Padding="0" 
                Command="{Binding RefreshClientCollectionCommand}" CommandParameter="{Binding ElementName=headWindow}"/>
        <!-- Кнопка для сохранения настроек -->
        <Button x:Name="save_btn" Content="Сохранить" HorizontalAlignment="Left" Margin="396,10,0,0" VerticalAlignment="Top" Width="110" RenderTransformOrigin="0.16,-0.227" Height="31" Background="{DynamicResource {x:Static SystemColors.GradientInactiveCaptionBrushKey}}"
                Command="{Binding SaveSettingsCommand}"/>
        <!-- Кнопка для сворачивания в трей -->
        <Button x:Name="hide_btn" Content="Свернуть" HorizontalAlignment="Left" Margin="396,46,0,0" VerticalAlignment="Top" Width="110" RenderTransformOrigin="0.16,-0.227" Height="31" Background="{DynamicResource {x:Static SystemColors.GradientInactiveCaptionBrushKey}}"
                Command="{Binding TaskBarCommand}" CommandParameter="{Binding ElementName=headWindow}"/>
        <!-- Кнопка для открытия окна автологина -->
        <Button x:Name="loging_btn" Content="Запуск" HorizontalAlignment="Left" Margin="511,10,0,0" VerticalAlignment="Top" Width="110" RenderTransformOrigin="0.16,-0.227" Height="31" Background="{DynamicResource {x:Static SystemColors.GradientInactiveCaptionBrushKey}}" 
                Command="{Binding OpenLoginWindowCommand}" CommandParameter="{Binding ElementName=headWindow}"/>
        <!-- Кнопка для открытия окна настройки оффсетов -->
        <Button x:Name="offset_btn" Content="Офсеты" HorizontalAlignment="Left" Margin="511,46,0,0" VerticalAlignment="Top" Width="110" RenderTransformOrigin="0.16,-0.227" Height="31" Background="{DynamicResource {x:Static SystemColors.GradientInactiveCaptionBrushKey}}" 
                Command="{Binding OpenOffsetWindowCommand}"/>
        <Grid x:Name="grid_loging" HorizontalAlignment="Left" Height="353" Margin="626,10,0,0" VerticalAlignment="Top" Width="247" Visibility="Hidden">
            <GroupBox x:Name="groupBox_logging" Header="Список аккаунтов" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="333" Width="227">
                <Grid>
                    <Grid Margin="0,0,0,66">
                        <ListBox x:Name="listAccount" Background="#F2CAD4DE" ItemsSource="{Binding Accounts}"
                                 ScrollViewer.VerticalScrollBarVisibility="Hidden" ScrollViewer.HorizontalScrollBarVisibility="Disabled" Style="{DynamicResource listBoxStyle1}">
                            <ListBox.ItemTemplate>
                                <DataTemplate>
                                    <StackPanel Orientation="Horizontal" Background="#F2CAD4DE" Height="25">
                                        <TextBlock Width="120" TextAlignment="Center" Text="{Binding Nickname}" Height="20" Background="#F2CAD4DD"></TextBlock>
                                        <TextBlock Width="60" TextAlignment="Center" Text="{Binding Server}" Margin="10, 0, 0, 0"  Height="20" Background="#F2CAD4DD"></TextBlock>
                                        <CheckBox VerticalAlignment="Center" IsChecked="True"></CheckBox>
                                        <StackPanel.ToolTip>
                                            <StackPanel Orientation="Horizontal">
                                                <Image Width="16" Height="16" Margin="5" Source="pack://application:,,,/Resources/food.png" Stretch="Fill"/>
                                                <TextBlock Height="16" Margin="5" Text="{Binding Food}"/>
                                                <Image Width="16" Height="16" Margin="5" Source="pack://application:,,,/Resources/iron.jpg" Stretch="Fill"/>
                                                <TextBlock Height="16" Margin="5" Text="{Binding Iron}"/>
                                                <Image Width="16" Height="16" Margin="5" Source="pack://application:,,,/Resources/tree.jpg" Stretch="Fill"/>
                                                <TextBlock Height="16" Margin="5" Text="{Binding Tree}"/>
                                                <Image Width="16" Height="16" Margin="5" Source="pack://application:,,,/Resources/rock.jpg" Stretch="Fill"/>
                                                <TextBlock Height="16" Margin="5" Text="{Binding Rock}"/>
                                                <Image Width="16" Height="16" Margin="5" Source="pack://application:,,,/Resources/cloth.jpg" Stretch="Fill"/>
                                                <TextBlock Height="16" Margin="5" Text="{Binding Cloth}"/>
                                            </StackPanel>
                                        </StackPanel.ToolTip>
                                    </StackPanel>
                                </DataTemplate>
                            </ListBox.ItemTemplate>
                        </ListBox>
                    </Grid>
                    <Grid Margin="0,259,0,10">
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                            <!--Кнопка удаления аккаунта-->
                            <Button Width="35" Height="35" BorderThickness="0" Focusable="False"  Margin="5, 0, 5, 0"
                                    Command="{Binding DeleteAccountCommand}" CommandParameter="{Binding ElementName=listAccount, Path=SelectedItem}"
                                    ToolTip="Удалить аккаунт">
                                <Button.ContentTemplate>
                                    <DataTemplate>
                                        <StackPanel Orientation="Horizontal">
                                            <Image Source="pack://application:,,,/Resources/delete.jpg" Stretch="Fill"/>
                                        </StackPanel>
                                    </DataTemplate>
                                </Button.ContentTemplate> />
                            </Button>
                            <!--Кнопка добавления аккаунта-->
                            <Button Width="35" Height="35" Margin="5, 0, 5, 0" BorderThickness="0" Focusable="False"
                                    Command="{Binding AddAccountCommand}" CommandParameter="{Binding ElementName=listAccount, Path=SelectedItem}"
                                    ToolTip="Добавить новый аккаунт">
                                <Button.ContentTemplate>
                                    <DataTemplate>
                                        <StackPanel Orientation="Horizontal">
                                            <Image Source="pack://application:,,,/Resources/add.jpg" Stretch="Fill"/>
                                        </StackPanel>
                                    </DataTemplate>
                                </Button.ContentTemplate> />
                            </Button>
                            <!--Кнопка настроек для авторизации-->
                            <Button Width="35" Height="35" Margin="5, 0, 5, 0" BorderThickness="0" Focusable="False"
                                    Command="{Binding LoginSettingsOpneCommand}" CommandParameter="{Binding ElementName=listAccount, Path=SelectedItem}"
                                    ToolTip="Настройки авторизации">
                                <Button.ContentTemplate>
                                    <DataTemplate>
                                        <StackPanel Orientation="Horizontal">
                                            <Image Source="pack://application:,,,/Resources/set.jpg" Stretch="Fill"/>
                                        </StackPanel>
                                    </DataTemplate>
                                </Button.ContentTemplate> />
                            </Button>
                            <!--Кнопка запуска клиента-->
                            <Button Width="35" Height="35" Margin="5, 0, 5, 0" BorderThickness="0" Focusable="False"
                                    Command="{Binding PlayCommand}" CommandParameter="{Binding ElementName=listAccount, Path=SelectedItem}"
                                    ToolTip="Запустить клиент">
                                <Button.ContentTemplate>
                                    <DataTemplate>
                                        <StackPanel Orientation="Horizontal">
                                            <Image Source="pack://application:,,,/Resources/play.jpg" Stretch="Fill"/>
                                        </StackPanel>
                                    </DataTemplate>
                                </Button.ContentTemplate> />
                            </Button>
                            <!--Кнопка запуска протыкивания домоквеста-->
                            <Button Width="35" Height="35" Margin="5, 0, 5, 0" BorderThickness="0" Focusable="False"
                                    Command="{Binding PlayCommand}" CommandParameter="{Binding ElementName=listAccount, Path=SelectedItem}"
                                    ToolTip="Сделать всем домоквест">
                                <Button.ContentTemplate>
                                    <DataTemplate>
                                        <StackPanel Orientation="Horizontal">
                                            <Image Source="pack://application:,,,/Resources/play_all.jpg" Stretch="Fill"/>
                                        </StackPanel>
                                    </DataTemplate>
                                </Button.ContentTemplate> />
                            </Button>
                        </StackPanel>
                    </Grid>
                </Grid>
            </GroupBox>
        </Grid>
    </Grid>
</Window>

