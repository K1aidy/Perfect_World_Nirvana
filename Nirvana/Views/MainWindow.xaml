<Window 
        x:Class="Nirvana.Views.MainWindow"
        x:Name="headWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:p="clr-namespace:Nirvana.Properties"
        xmlns:local="clr-namespace:Nirvana.Views"
        xmlns:commands="clr-namespace:Nirvana.Models.TaskBar"
        xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
        xmlns:tb="http://www.hardcodet.net/taskbar"
        mc:Ignorable="d"
        Top="{Binding Source={x:Static p:Settings.Default}, Path=Top, Mode=TwoWay}"
        Left="{Binding Source={x:Static p:Settings.Default}, Path=Left, Mode=TwoWay}"
        Height="{Binding Source={x:Static p:Settings.Default}, Path=Height, Mode=TwoWay}"
        Width="640"
        Title="НирванаБот" WindowStyle="None" ResizeMode="NoResize" AllowsTransparency="True"   ShowInTaskbar="False" >

    <Window.Resources>
        <Style x:Key="listBoxStyle1" TargetType="{x:Type ListBox}">
            <Style.Resources>
                <SolidColorBrush x:Key="{x:Static SystemColors.HighlightBrushKey}" Color="Transparent" />
            </Style.Resources>
        </Style>
    </Window.Resources>
    <i:Interaction.Triggers>
        <i:EventTrigger EventName="MouseLeftButtonDown">
            <i:InvokeCommandAction
                            Command="{Binding DragMoveCommand}"
                            CommandParameter="{Binding ElementName=headWindow}" />
        </i:EventTrigger>
        <i:EventTrigger EventName="Loaded">
            <i:InvokeCommandAction
                            Command="{Binding LoadedCommand}"
                            CommandParameter="{Binding ElementName=headWindow}" />
        </i:EventTrigger>
    </i:Interaction.Triggers>
    <Window.Foreground>
        <SolidColorBrush Color="White"/>
    </Window.Foreground>
    <Window.Background>
        <SolidColorBrush Color="#F2CAD4DE"/>
    </Window.Background>

    <Grid>
        <tb:TaskbarIcon
            x:Name="tbi" VerticalAlignment="Top"  LeftClickCommand="{Binding TaskBarCommand}" LeftClickCommandParameter="{Binding ElementName=headWindow}" IconSource="/Nirvana;component/Resources/258211.ico">
            <tb:TaskbarIcon.ContextMenu>
                <ContextMenu
                    Background="White">
                    <MenuItem Header="Развернуть" Command="{commands:ShowSampleWindowCommand}" CommandParameter="{Binding}"></MenuItem>
                    <MenuItem Header="Свернуть" Command="{commands:HideSampleWindowCommand}" CommandParameter="{Binding}"></MenuItem>
                    <Separator/>
                    <MenuItem Header="Выход" Command="{commands:CloseWindowCommand}" CommandParameter="{Binding}">
                        <MenuItem.Icon>
                            <Image Width="16" Height="16" Source="pack://application:,,,/Resources/close.jpg"/>
                        </MenuItem.Icon>
                    </MenuItem>
                </ContextMenu>
            </tb:TaskbarIcon.ContextMenu>

        </tb:TaskbarIcon>
